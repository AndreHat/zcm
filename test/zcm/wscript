#! /usr/bin/env python
# encoding: utf-8

def build(ctx):
    ctx.program(target = 'sub_unsub',
                use = 'default zcm',
                source = 'sub_unsub.cpp',
                install_path = None)

    ctx.program(target = 'sub_unsub_static',
                use = 'default zcmstatic',
                source = 'sub_unsub.cpp',
                install_path = None)
